{
  "name": "pro-store",
  "version": "1.8.2",
  "private": true,
  "author": "Bora Lee (https://github.com/bora001)",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "dev:w": "turbo run dev --filter=@pro-store-monorepo/web",
    "dev:s": "turbo run dev --filter=@pro-store-monorepo/websocket",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^20.17.24",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "turbo": "2.5.0"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "next": "workspace:*"
  },
  "pnpm": {
    "overrides": {
      "next": "15.2.4",
      "prismjs": "^1.30.0",
      "brace-expansion": ">=1.1.12"
    }
  }
}
